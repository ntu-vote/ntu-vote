{
  "name": "ntu-vote",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/bin/server.ts",
    "build": "tsc&&cp -RT public dist/public && cp package.json dist/package.json",
    "dev": "npx nodemon -e ts,tsx --exec ts-node bin/server.ts",
    "test": "echo \"Error: no test specified\" && exit 0",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typeorm-sync": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.json migration:generate -n",
    "typeorm-run": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.json migration:run",
    "typeorm-revert": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.json migration:revert"
  },
  "dependencies": {
    "cron": "^1.8.2",
    "debug": "^4.1.1",
    "ioredis": "^4.27.6",
    "ioredis-mock": "^5.6.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-combine-routers": "^4.0.2",
    "koa-generic-session": "^2.1.1",
    "koa-json": "^2.0.2",
    "koa-redis": "^4.0.1",
    "koa-router": "^9.4.0",
    "koa-static": "^5.0.0",
    "mysql2": "^2.2.5",
    "node-fetch": "^2.6.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.32",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "@types/cron": "^1.7.2",
    "@types/debug": "^4.1.5",
    "@types/ioredis": "^4.26.0",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-generic-session": "^1.0.4",
    "@types/koa-json": "^2.0.18",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.1",
    "@types/koa-static": "^4.0.1",
    "@types/node": "^14.14.37",
    "@types/node-fetch": "^2.5.7",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
