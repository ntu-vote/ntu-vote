{
  "name": "ntu-vote",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/bin/server.js",
    "build": "tsc&&cp -RT public dist/public && cp package.json dist/package.json",
    "dev": "npx nodemon -e ts,tsx --exec ts-node bin/server.ts",
    "test": "echo \"Error: no test specified\" && exit 0",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typeorm-sync": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.json migration:generate -n",
    "typeorm-run": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.json migration:run",
    "typeorm-revert": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.json migration:revert"
  },
  "dependencies": {
    "@koa/cors": "3.1.0",
    "bcryptjs": "2.4.3",
    "cron": "1.8.2",
    "debug": "4.3.2",
    "ioredis": "4.28.0",
    "ioredis-mock": "5.6.0",
    "jsonwebtoken": "8.5.1",
    "koa": "2.13.4",
    "koa-bodyparser": "4.3.0",
    "koa-combine-routers": "4.0.2",
    "koa-generic-session": "2.2.0",
    "koa-json": "2.0.2",
    "koa-redis": "4.0.1",
    "koa-router": "9.4.0",
    "koa-static": "5.0.0",
    "node-fetch": "2.6.5",
    "pg": "8.7.1",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.2.38",
    "validator": "13.6.0"
  },
  "devDependencies": {
    "@types/cron": "1.7.3",
    "@types/debug": "4.1.7",
    "@types/ioredis": "4.27.8",
    "@types/koa": "2.13.4",
    "@types/koa__cors": "3.0.3",
    "@types/koa-bodyparser": "4.3.3",
    "@types/koa-generic-session": "1.0.6",
    "@types/koa-json": "2.0.20",
    "@types/koa-logger": "3.1.2",
    "@types/koa-router": "7.4.4",
    "@types/koa-static": "4.0.2",
    "@types/node": "14.17.27",
    "@types/node-fetch": "2.5.12",
    "@types/validator": "13.6.4",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "nodemon": "2.0.14",
    "ts-node": "9.1.1",
    "typescript": "4.4.4"
  }
}
